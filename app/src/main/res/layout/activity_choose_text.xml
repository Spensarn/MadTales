<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".choose_text"
        android:background="?android:attr/windowBackground">

    <LinearLayout
            android:orientation="vertical"
            android:layout_width="368dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent">
        <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            <android.support.constraint.Guideline
                    android:orientation="vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" app:layout_constraintGuide_begin="184dp"
                    android:id="@+id/guideline13" app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toEndOf="parent"/>
            <android.support.constraint.Guideline
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" app:layout_constraintGuide_begin="74dp"
                    android:id="@+id/guideline14"/>
            <android.support.constraint.Guideline
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" app:layout_constraintGuide_begin="180dp"
                    android:id="@+id/guideline15"/>
            <android.support.constraint.Guideline
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" app:layout_constraintGuide_begin="286dp"
                    android:id="@+id/guideline16"/>
            <android.support.constraint.Guideline
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" app:layout_constraintGuide_begin="392dp"
                    android:id="@+id/guideline17"/>
            <Button
                    android:text="Presskonferens"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:id="@+id/presskonferens_button" app:layout_constraintEnd_toStartOf="@+id/guideline13"
                    android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
                    android:layout_marginStart="8dp" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="@+id/guideline14" android:layout_marginBottom="8dp"
                    app:layout_constraintBottom_toTopOf="@+id/guideline15"
                    android:background="@android:color/holo_blue_light"/>
            <Button
                    android:text="Intervju"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:id="@+id/jobbintervju_button" app:layout_constraintStart_toStartOf="@+id/guideline13"
                    android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
                    app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                    android:layout_marginRight="8dp" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="@+id/guideline14" android:layout_marginBottom="8dp"
                    app:layout_constraintBottom_toTopOf="@+id/guideline15"
                    android:background="@android:color/holo_green_light"/>
            <TextView
                    android:text="Välj Text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView4" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                    android:layout_marginRight="8dp" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginLeft="8dp" android:layout_marginStart="8dp" android:layout_marginBottom="8dp"
                    app:layout_constraintBottom_toTopOf="@+id/guideline14" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="parent" android:textStyle="bold" android:textAllCaps="true"
                    app:fontFamily="serif-monospace" android:textSize="18sp"/>
            <Button
                    android:text="Julspecial"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:id="@+id/julspecial_button" app:layout_constraintEnd_toStartOf="@+id/guideline13"
                    android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
                    android:layout_marginStart="8dp" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="@+id/guideline15" android:layout_marginBottom="8dp"
                    app:layout_constraintBottom_toTopOf="@+id/guideline16"
                    android:background="@android:color/holo_red_light"/>
            <Button
                    android:text="Saga"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:id="@+id/saga_button"
                    app:layout_constraintStart_toStartOf="@+id/guideline13" android:layout_marginLeft="8dp"
                    android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp" android:layout_marginRight="8dp" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="@+id/guideline15" android:layout_marginBottom="8dp"
                    app:layout_constraintBottom_toTopOf="@+id/guideline16"
                    android:background="@android:color/holo_purple" android:textAllCaps="true"
                    android:textColor="@color/cardview_light_background"/>
            <Button
                    android:text="Bankrån"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:id="@+id/bankrån_button" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginLeft="8dp" android:layout_marginStart="8dp" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="@+id/guideline16"
                    app:layout_constraintEnd_toStartOf="@+id/guideline13" android:layout_marginEnd="8dp"
                    android:layout_marginRight="8dp" android:layout_marginBottom="8dp"
                    app:layout_constraintBottom_toTopOf="@+id/guideline17"
                    android:background="@color/colorAccent" android:textColor="@color/cardview_light_background"
            />
            <Button
                    android:text="Kommer SNart!"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:id="@+id/button27" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                    android:layout_marginRight="8dp" app:layout_constraintStart_toStartOf="@+id/guideline13"
                    android:layout_marginLeft="8dp" android:layout_marginStart="8dp" android:layout_marginBottom="8dp"
                    app:layout_constraintBottom_toTopOf="@+id/guideline17" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="@+id/guideline16"
                    android:background="@android:color/darker_gray" android:textStyle="italic"
                    android:textColor="@android:color/black"/>
            <ImageButton
                    android:layout_width="34dp"
                    android:layout_height="38dp" app:srcCompat="@drawable/ic_keyboard_arrow_left_orange_24dp"
                    android:background="@android:color/transparent"
                    android:id="@+id/back_choose_text"
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="@+id/guideline17" android:layout_marginBottom="8dp"
                    app:layout_constraintBottom_toBottomOf="parent" android:layout_marginEnd="8dp"
                    app:layout_constraintEnd_toStartOf="@+id/guideline13" android:layout_marginRight="8dp"
                    android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginLeft="8dp" app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintVertical_bias="0.97"/>
            <ImageButton
                    android:layout_width="wrap_content"
                    android:background="@android:color/transparent"
                    android:layout_height="wrap_content" app:srcCompat="@drawable/ic_done_black_24dp"
                    android:id="@+id/imageButton2" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="@+id/guideline17" android:layout_marginBottom="8dp"
                    app:layout_constraintBottom_toBottomOf="parent" android:layout_marginEnd="8dp"
                    app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp"
                    android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="@+id/guideline13"
                    android:layout_marginLeft="8dp" app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintVertical_bias="0.971" android:visibility="invisible"/>
        </android.support.constraint.ConstraintLayout>
    </LinearLayout>
</android.support.constraint.ConstraintLayout>